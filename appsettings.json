{
  "ConnectionStrings": {
    //ConnectionStrings
  },

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JWT": {
    "Issuer": "SecureApi", //url for host
    "Audience": "SecureApiUser", //Domin name
    "Key": "",
    "DurationInDays": 30
  },

  "IdentityOptions": {
    "Password": {
      "RequiredLength": 6,
      "RequiredUniqueChars": 6,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": true,
      "RequireLowercase": true,
      "RequireDigit": true
    }
  }
  }
